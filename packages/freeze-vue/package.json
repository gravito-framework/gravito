{
  "name": "@gravito/freeze-vue",
  "version": "1.0.0-beta.6",
  "description": "Vue adapter for @gravito/freeze SSG module",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "bun tsc --noEmit --skipLibCheck --incremental false",
    "test": "bun test",
    "test:coverage": "bun test --coverage --coverage-threshold=80",
    "test:ci": "bun test --coverage --coverage-threshold=80"
  },
  "keywords": [
    "gravito",
    "ssg",
    "vue",
    "static-site",
    "inertia"
  ],
  "author": "Carl Lee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gravito-framework/gravito.git",
    "directory": "packages/freeze-vue"
  },
  "peerDependencies": {
    "vue": ">=3.0.0",
    "@inertiajs/vue3": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@inertiajs/vue3": {
      "optional": true
    }
  },
  "dependencies": {
    "@gravito/freeze": "workspace:*"
  },
  "devDependencies": {
    "vue": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "bun-types": "latest"
  }
}