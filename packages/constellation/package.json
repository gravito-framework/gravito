{
  "name": "@gravito/constellation",
  "version": "1.0.0-beta.1",
  "description": "Powerful sitemap generation for Gravito applications with dynamic/static support, sharding, and caching.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "typecheck": "bun tsc -p tsconfig.json --noEmit --skipLibCheck",
    "test": "bun test",
    "test:coverage": "bun test --coverage --coverage-threshold=80",
    "test:ci": "bun test --coverage --coverage-threshold=80"
  },
  "peerDependencies": {
    "@gravito/core": "workspace:*"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.956.0",
    "@google-cloud/storage": "^7.18.0",
    "@gravito/stream": "workspace:*",
    "@gravito/photon": "workspace:*"
  },
  "devDependencies": {
    "bun-plugin-dts": "^0.3.0",
    "bun-types": "latest",
    "@gravito/core": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gravito-framework/gravito.git",
    "directory": "packages/constellation"
  },
  "license": "MIT",
  "author": "Carl Lee <carllee0520@gmail.com>",
  "homepage": "https://github.com/gravito-framework/gravito#readme"
}