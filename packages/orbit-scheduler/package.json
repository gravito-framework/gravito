{
  "name": "@gravito/orbit-scheduler",
  "version": "1.0.0-alpha.1",
  "description": "Distributed task scheduler for Gravito framework",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
