  "dependencies": {},
  "optionalDependencies": {
    "cron-parser": "^4.9.0"
  },
  "peerDependencies": {
    "gravito-core": "1.0.0-beta.1",
    "@gravito/orbit-cache": "1.0.0-beta.1",
    "hono": "^4.11.1"
  },
  "devDependencies": {
    "gravito-core": "1.0.0-beta.1",
    "@gravito/orbit-cache": "1.0.0-beta.1",
    "@types/bun": "latest",
    "typescript": "^5.9.3",
    "hono": "^4.11.1"
  },
  "publishConfig": {
    "access": "public"
  }
