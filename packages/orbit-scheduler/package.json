{
    "name": "@gravito/orbit-scheduler",
    "version": "0.3.0-alpha.1",
    "description": "Distributed task scheduler for Gravito framework",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "bun run build.ts",
        "test": "bun test",
        "lint": "biome lint ./src",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "gravito",
        "orbit",
        "scheduler",
        "cron",
        "task"
    ],
    "author": "Carl Lee <carllee0520@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "cron-parser": "^4.9.0"
    },
    "peerDependencies": {
        "gravito-core": "workspace:*",
        "@gravito/orbit-cache": "workspace:*",
        "hono": "^4.11.1"
    },
    "devDependencies": {
        "gravito-core": "workspace:*",
        "@gravito/orbit-cache": "workspace:*",
        "@types/bun": "latest",
        "typescript": "^5.9.3",
        "hono": "^4.11.1"
    }
}