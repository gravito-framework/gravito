{
  "name": "@gravito/freeze",
  "version": "1.0.0-beta.6",
  "description": "Static Site Generation (SSG) core module for Gravito Framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "bun tsc --noEmit --skipLibCheck --incremental false",
    "test": "bun test",
    "test:coverage": "bun test --coverage --coverage-threshold=80",
    "test:ci": "bun test --coverage --coverage-threshold=80"
  },
  "keywords": [
    "gravito",
    "ssg",
    "static-site",
    "static-site-generation",
    "jamstack"
  ],
  "author": "Carl Lee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gravito-framework/gravito.git",
    "directory": "packages/freeze"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "bun-types": "latest"
  }
}