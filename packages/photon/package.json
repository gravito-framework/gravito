{
  "name": "@gravito/photon",
  "version": "1.0.0-beta.1",
  "description": "Gravito Photon HTTP engine (compat wrapper)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "default": "./dist/logger.js"
    },
    "./bun": {
      "types": "./dist/bun.d.ts",
      "default": "./dist/bun.js"
    },
    "./jwt": {
      "types": "./dist/jwt.d.ts",
      "default": "./dist/jwt.js"
    },
    "./http-exception": {
      "types": "./dist/http-exception.d.ts",
      "default": "./dist/http-exception.js"
    },
    "./router/reg-exp-router": {
      "types": "./dist/router/reg-exp-router.d.ts",
      "default": "./dist/router/reg-exp-router.js"
    },
    "./router/trie-router": {
      "types": "./dist/router/trie-router.d.ts",
      "default": "./dist/router/trie-router.js"
    }
  },
  "scripts": {
    "build": "bun run build.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage --coverage-threshold=80",
    "test:ci": "bun test --coverage --coverage-threshold=80"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gravito-framework/gravito.git",
    "directory": "packages/photon"
  }
}
