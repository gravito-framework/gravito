{
  "name": "@gravito/scaffold",
  "version": "1.0.0-beta.1",
  "description": "Project scaffolding engine for Gravito - Generate enterprise-grade architecture templates",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "test:coverage": "bun test --coverage --coverage-threshold=80",
    "test:ci": "bun test --coverage --coverage-threshold=80"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "gravito-core": "1.0.0-beta.6"
  },
  "author": "Carl Lee <carllee0520@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/gravito-framework/gravito#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gravito-framework/gravito.git",
    "directory": "packages/scaffold"
  }
}
