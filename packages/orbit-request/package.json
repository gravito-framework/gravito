{
    "name": "@gravito/orbit-request",
    "version": "0.1.0-alpha.0",
    "description": "Form Request validation for Gravito - Laravel-style request validation with Zod",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "bun run build.ts",
        "test": "bun test",
        "prepublishOnly": "bun run build"
