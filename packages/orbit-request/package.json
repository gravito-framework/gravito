    "peerDependencies": {
        "hono": "^4.0.0"
    },
    "devDependencies": {
        "@types/bun": "^1.1.0",
        "typescript": "^5.0.0"
    },
    "author": "Carl Lee",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/CarlLee1983/gravito.git",
        "directory": "packages/orbit-request"
