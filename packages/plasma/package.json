{
  "name": "@gravito/plasma",
  "version": "1.0.0-beta.6",
  "description": "Redis client for Gravito - Bun native, Laravel-style API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "node_modules/.bin/tsup src/index.ts --format esm,cjs --dts || bunx tsup src/index.ts --format esm,cjs --dts",
    "test": "bun test",
    "typecheck": "bun tsc -p tsconfig.json --noEmit --skipLibCheck"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "gravito",
    "redis",
    "cache",
    "bun",
    "key-value"
  ],
  "author": "Gravito Team",
  "license": "MIT",
  "peerDependencies": {
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "bun-types": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gravito-framework/gravito.git",
    "directory": "packages/plasma"
  }
}
